{
  "manifest_version": 3,
  "name": "Vzota",
  "version": "1.0.0",
  "description": "Mở khóa tính năng VIP, point không giới hạn cho Azota",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "key": "",
  "content_scripts": [
    {
      "matches": ["https://azota.vn/*"],
      "js": ["assets/content.js"],
      "run_at": "document_start",
      "world": "MAIN"
    }
  ],
  "host_permissions": ["https://azota.vn/*"]
}
